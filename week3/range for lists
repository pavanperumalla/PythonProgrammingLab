def creat_lists(l,n):
	for i in range(n):
		temp=int(input())
		l.append(temp)
	return l
	
l1=[]
l2=[]
l3=[]
n1=int(input("enter the elements"))
n2=int(input("enter the elements"))
n3=int(input("enter the elements"))

print("enter elements in first list")
l1=creat_lists(l1,n1)
print(l1)
  
print("enter the elements in second list")
l2=creat_lists(l2,n2)
print(l2)

print("enter the elements in third list")
l3=creat_lists(l3,n3) 
print(l3)
