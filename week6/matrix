r=int(input("enter the size: "))
c=int(input("enter the size: "))
matrix=[]
for i in range(r):
    r=[]
    for j in range(c):
        n=int(input (f"enter the elements in matrix{[i]}{[j]} :"))
        r.append(n)
    matrix.append(r)
