mul=[]
print("it is multiplication")
for i in range(c):
    row=[]
    for j in range(r):
        temp=0
        for k in range(r):
            temp+= matrix[i][k]*matrix1[k][j]
        row.append(temp)
    mul.append(row)
print(mul)  








